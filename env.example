# OpenAI/OpenRouter API Configuration
# You must set at least one of OPENROUTER_API_KEY or OPENAI_API_KEY

# OpenRouter API Key (recommended for model flexibility)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# OR OpenAI API Key (direct OpenAI access)
# OPENAI_API_KEY=your_openai_api_key_here

# Optional: Default model to use (if not specified in command)
DEFAULT_MODEL=gpt-3.5-turbo

# Optional: OpenRouter headers (for analytics/attribution)
OPENROUTER_APP_NAME=Customer Support Helper
